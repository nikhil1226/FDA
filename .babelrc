{
  "presets": [
    "react",
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-1"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-1"]
    }
  },
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "transform-regenerator",
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}
